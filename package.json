{
  "name": "ac-auto",
  "version": "1.0.6",
  "description": "Aho-Corasick automaton implementation for fast string matching.",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE",
    "AUTHORS"
  ],
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "clean": "rimraf ./dist/ ./tsconfig.build.tsbuildinfo",
    "compile": "tsc -p tsconfig.json",
    "build": "npm run clean && npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**.ts",
    "prettier:check": "prettier -l \"src/**/*.ts\" \"src/**/*.js\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "bench": "node --expose-gc benchmarks/index.bench.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronjan/fastscan.git"
  },
  "keywords": [
    "ahocorasick",
    "aho-corasick",
    "javascript",
    "typescript",
    "node"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aaronjan/ac-auto/issues"
  },
  "homepage": "https://github.com/aaronjan/ac-auto#readme",
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "mocha": "^5.2.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  }
}
